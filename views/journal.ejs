<!doctype html>
<html>

<head>
  <title>21 Savage Fan Site</title>
  <link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.0.2/css/bootstrap.min.css">
  <link rel="stylesheet" href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.min.css">
  <link rel="stylesheet" href="style.css">
  <script src="https://kit.fontawesome.com/47aae21c46.js" crossorigin="anonymous"></script>
</head>

<body>
  <div class="container">

    <div class="page-header text-center">
      <h1><span class="fa fa-anchor"></span> Entries</h1>
      <a href="/logout" class="btn btn-default btn-sm">Logout</a>
      <a href="/profile" class="btn btn-default btn-sm">Home</a>
    </div>

    <div class="row">
      <% for (let i=0; i < entries.length; i++) {%>
        <ul>
          <li><p><span><%= entries[i].date %> </span><%= entries[i].entry %> <i class="fa-solid fa-circle-minus"></i></p></li>
        </ul>
      <% } %>   
    </div>

  </div>

  <script src="main.js"></script>

</body>

</html>